- python-dev:
   python_dev_version: 2.7
   python_dev_setup: "pip"
   python_dev_requirements_files:
      - requirements_dev.txt
   packages:
      - pycrypto-related:
          pkgs:
            debian:
              - libssl-dev
              - libffi-dev
              - libsqlite3-dev
            conda:
              - cryptography
              - pycrypto
            other: omit

- vagrant-dev:
    vagrant_dev_providers:
      - virtualbox
